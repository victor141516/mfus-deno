{
  "name": "mfus-node",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node --loader ts-node/esm src/main.ts",
    "build": "tsc",
    "run-compiled": "node",
    "lint": "eslint src/**",
    "lint:tsc": "tsc --noEmit"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module-scoped"
    }
  },
  "eslintIgnore": [
    "/**/*.js"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.27",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}